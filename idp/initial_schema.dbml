    Indexes {
        (email) [unique, name: 'accounts_email_uidx']
        (public_id) [unique, name: 'accounts_public_id_uidx']
        (public_id, version) [name: 'accounts_public_id_version_idx']
        (username) [unique, name: 'accounts_username_uidx']
    }
}

